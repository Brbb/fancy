{
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-console": "off",
    "indent": ["error", "tab"],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"]
  }
}
